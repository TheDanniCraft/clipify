{
	"name": "clipify",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"app:dev": "infisical run --env=dev --watch -- next dev --webpack",
		"app:webhook": "stripe listen --forward-to localhost:3000/payment/webhook",
		"app:eventsub": "ngrok http 3000 --url https://well-free-wildcat.ngrok-free.app",
		"app:build": "next build --webpack",
		"app:start": "next start",
		"app:lint": "eslint .",
		"db:push": "infisical run --env=dev -- drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "infisical run --env=dev -- drizzle-kit studio",
		"prepare": "next-ws patch"
	},
	"dependencies": {
		"@heroui/react": "^2.8.7",
		"@sentry/nextjs": "^10.32.1",
		"@tabler/icons-react": "^3.36.1",
		"axios": "^1.13.2",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.45.1",
		"jsonwebtoken": "^9.0.3",
		"minimatch": "^10.1.1",
		"motion": "^12.24.0",
		"next": "^16.1.1",
		"next-navigation-guard": "^0.2.0",
		"next-plausible": "^3.12.5",
		"next-themes": "^0.4.6",
		"next-ws": "^2.1.12",
		"pg": "^8.16.3",
		"rate-limiter-flexible": "^9.0.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"stripe": "^20.1.0",
		"ws": "^8.18.3",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4.1.18",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.0.3",
		"@types/pg": "^8.16.0",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/ws": "^8.18.1",
		"@vercel/nft": "^1.1.1",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9.39.2",
		"eslint-config-next": "^16.1.1",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"patchedDependencies": {
		"next@16.1.0": "patches/next@16.1.0.patch"
	}
}
