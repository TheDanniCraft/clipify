{
	"name": "clipify",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"app:dev": "infisical run --env=dev --watch -- next dev --webpack",
		"app:webhook": "stripe listen --forward-to localhost:3000/payment/webhook",
		"app:eventsub": "ngrok http 3000 --url https://well-free-wildcat.ngrok-free.app",
		"app:build": "next build --webpack",
		"app:start": "next start",
		"app:lint": "eslint .",
		"db:push": "infisical run --env=dev -- drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "infisical run --env=dev -- drizzle-kit studio",
		"prepare": "next-ws patch"
	},
	"dependencies": {
		"@heroui/react": "^2.8.8",
		"@sentry/nextjs": "^10.38.0",
		"@tabler/icons-react": "^3.36.1",
		"axios": "^1.13.4",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.45.1",
		"jsonwebtoken": "^9.0.3",
		"minimatch": "^10.1.2",
		"motion": "^12.31.0",
		"next": "^16.1.6",
		"next-navigation-guard": "^0.2.0",
		"next-plausible": "^3.12.5",
		"next-themes": "^0.4.6",
		"next-ws": "^2.1.16",
		"nextjs-turnstile": "^1.0.3",
		"pg": "^8.18.0",
		"rate-limiter-flexible": "^9.1.0",
		"re2js": "^1.2.1",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"stripe": "^20.3.0",
		"ws": "^8.19.0",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4.1.18",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.2.0",
		"@types/pg": "^8.16.0",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@types/ws": "^8.18.1",
		"@vercel/nft": "^1.3.0",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9.39.2",
		"eslint-config-next": "^16.1.6",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"patchedDependencies": {
		"next@16.1.6": "patches/next@16.1.6.patch"
	}
}
