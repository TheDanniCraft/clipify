{
	"name": "clipify",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"app:dev": "infisical run --env=dev --watch -- next dev",
		"app:webhook": "stripe listen --forward-to localhost:3000/payment/webhook",
		"app:eventsub": "ngrok http 3000 --url https://well-free-wildcat.ngrok-free.app",
		"app:build": "next build",
		"app:start": "next start",
		"app:lint": "eslint .",
		"db:push": "infisical run --env=dev -- drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "infisical run --env=dev -- drizzle-kit studio",
		"prepare": "next-ws patch"
	},
	"dependencies": {
		"@heroui/react": "^2.8.5",
		"@sentry/nextjs": "^10.27.0",
		"@tabler/icons-react": "^3.35.0",
		"axios": "^1.13.2",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.7",
		"jsonwebtoken": "^9.0.2",
		"motion": "^12.23.24",
		"next": "^15.5.6",
		"next-navigation-guard": "^0.2.0",
		"next-plausible": "^3.12.5",
		"next-themes": "^0.4.6",
		"next-ws": "^2.1.7",
		"pg": "^8.16.3",
		"rate-limiter-flexible": "^8.3.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"stripe": "^18.5.0",
		"ws": "^8.18.3",
		"xss": "^1.0.15"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.1",
		"@types/pg": "^8.15.6",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/ws": "^8.18.1",
		"@vercel/nft": "^0.30.4",
		"drizzle-kit": "^0.31.7",
		"eslint": "^9.39.1",
		"eslint-config-next": "15.5.4",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	}
}
